{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Default",
    "compilerOptions": {
        "module": "ESNext",
        "target": "ES2022",
        "lib": [
            "ESNext",
            "Decorators"
        ],
        "allowSyntheticDefaultImports": true,
        "noImplicitThis": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": false,
        "sourceMap": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "moduleResolution": "Bundler",
        "strict": true,
        "noEmit": true
    },
    "exclude": [
        "../../node_modules",
        "../../**/node_modules"
    ]
}